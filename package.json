{
  "name": "webapp",
  "version": "1.0.0",
  "description": "Web app for search around the medicine cabinet api.",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch webpack:watch",
    "security-check": "nsp check",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "webpack": "webpack",
    "webpack:watch": "npm run webpack -- --watch",
    "build": "webpack -p",
    "deploy": "http-server -p 8080"
  },
  "author": "ofirelarat",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.6",
    "angular-route": "^1.6.6",
    "bootstrap": "^3.3.7"
  },
  "devDependencies": {
    "webpack": "^1.12.9",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "nsp": "2.6.2",
    "npm-run-all": "3.1.1"
  }
}
